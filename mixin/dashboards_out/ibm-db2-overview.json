{
      "annotations": {
         "list": [ ]
      },
      "editable": false,
      "id": null,
      "links": [
         {
            "keepTime": true,
            "title": "IBM DB2 logs",
            "type": "link",
            "url": "/d/ibmdb2-logs"
         }
      ],
      "panels": [
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 0
            },
            "id": 1,
            "panels": [ ],
            "title": "Overview",
            "type": "row"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "Whether the agent integration is up for this database.",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "thresholds"
                  },
                  "mappings": [
                     {
                        "options": {
                           "0": {
                              "color": "red",
                              "index": 0,
                              "text": "Not OK"
                           },
                           "1": {
                              "color": "green",
                              "index": 1,
                              "text": "OK"
                           }
                        },
                        "type": "value"
                     }
                  ]
               }
            },
            "gridPos": {
               "h": 6,
               "w": 6,
               "x": 0,
               "y": 1
            },
            "id": 2,
            "options": {
               "colorMode": "value",
               "graphMode": "none",
               "reduceOptions": {
                  "calcs": [
                     "lastNotNull"
                  ]
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "ibm_db2_up{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}",
                  "format": "time_series",
                  "instant": false,
                  "intervalFactor": 2,
                  "legendFormat": "{{instance}}",
                  "refId": "Up status"
               }
            ],
            "title": "Up status",
            "type": "stat"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The amount of active connections to the database.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never"
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 6,
               "w": 18,
               "x": 6,
               "y": 1
            },
            "id": 3,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "ibm_db2_application_active{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}",
                  "format": "time_series",
                  "instant": false,
                  "intervalFactor": 2,
                  "legendFormat": "{{database_name}}",
                  "refId": "Active connections"
               }
            ],
            "title": "Active connections",
            "type": "timeseries"
         },
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 7
            },
            "id": 4,
            "panels": [ ],
            "title": "Operations",
            "type": "row"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The number of row operations that are being performed on the database.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never"
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 6,
               "w": 12,
               "x": 0,
               "y": 8
            },
            "id": 5,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "increase(ibm_db2_row_total{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}[$__interval:] offset -$__interval)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "intervalFactor": 2,
                  "legendFormat": "{{database_name}} - {{row_state}}",
                  "refId": "Row operations"
               }
            ],
            "title": "Row operations",
            "type": "timeseries"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The percentage of time that the database manager did not need to load a page from disk to service a page request.",
