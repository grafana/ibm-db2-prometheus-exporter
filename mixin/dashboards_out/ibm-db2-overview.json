            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "ibm_db2_bufferpool_hit_ratio{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}",
                  "format": "time_series",
                  "instant": false,
                  "intervalFactor": 2,
                  "legendFormat": "{{database_name}} - {{bufferpool_name}}",
                  "refId": "Bufferpool hit ratio"
               }
            ],
            "title": "Bufferpool hit ratio",
            "type": "timeseries"
         },
         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
