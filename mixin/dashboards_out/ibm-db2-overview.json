         {
            "collapsed": false,
            "gridPos": {
               "h": 1,
               "w": 0,
               "x": 0,
               "y": 28
            },
            "id": 13,
            "panels": [ ],
            "title": "Logs",
            "type": "row"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The percentage of allocated storage being used by the IBM DB2 instance.",
            "fieldConfig": {
               "defaults": {
                  "color": {
                     "fixedColor": "text",
                     "mode": "thresholds"
                  },
                  "unit": "percent"
               }
            },
            "gridPos": {
               "h": 6,
               "w": 6,
               "x": 0,
               "y": 29
            },
            "id": 14,
            "options": {
               "colorMode": "value",
               "graphMode": "none",
               "reduceOptions": {
                  "calcs": [
                     "lastNotNull"
                  ]
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "100 * sum(ibm_db2_log_usage{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\", log_usage_type=\"used\"}) by (instance, job, database_name) / sum(ibm_db2_log_usage{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\", log_usage_type=\"available\"}) by (instance, job, database_name)",
                  "format": "time_series",
                  "instant": false,
                  "intervalFactor": 2,
                  "legendFormat": "{{instance}}",
                  "refId": "Log storage usage"
               }
            ],
            "title": "Log storage usage",
            "type": "stat"
         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The number of log pages read and written to by the logger.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never"
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 6,
               "w": 18,
               "x": 6,
               "y": 29
            },
            "id": 15,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
