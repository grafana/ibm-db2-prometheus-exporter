                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never"
                  },
                  "thresholds": {
                     "steps": [
                        {
                           "color": "green",
                           "value": null
                        },
                        {
                           "color": "red",
                           "value": 80
                        }
                     ]
                  },
                  "unit": "none"
               }
            },
            "gridPos": {
               "h": 6,
               "w": 8,
               "x": 8,
               "y": 22
            },
            "id": 11,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "increase(ibm_db2_deadlock_total{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}[$__interval:] offset -$__interval)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "intervalFactor": 2,
                  "legendFormat": "{{database_name}}",
                  "refId": "Deadlocks"
               }
            ],
            "title": "Deadlocks",
