         },
         {
            "datasource": {
               "type": "prometheus",
               "uid": "${prometheus_datasource}"
            },
            "description": "The number of locks active and waiting in use in the database.",
            "fieldConfig": {
               "defaults": {
                  "custom": {
                     "fillOpacity": 30,
                     "gradientMode": "opacity",
                     "lineInterpolation": "smooth",
                     "lineWidth": 2,
                     "showPoints": "never"
                  },
                  "unit": "none"
               },
               "overrides": [
                  {
                     "matcher": {
                        "id": "byRegexp",
                        "options": "/./"
                     },
                     "properties": [
                        {
                           "id": "custom.axisSoftMax",
                           "value": -4
                        }
                     ]
                  }
               ]
            },
            "gridPos": {
               "h": 6,
               "w": 8,
               "x": 16,
               "y": 22
            },
            "id": 12,
            "options": {
               "legend": {
                  "calcs": [ ],
                  "displayMode": "list"
               },
               "tooltip": {
                  "mode": "multi",
                  "sort": "desc"
               }
            },
            "pluginVersion": "v11.0.0",
            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "ibm_db2_lock_usage{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}",
                  "format": "time_series",
                  "instant": false,
