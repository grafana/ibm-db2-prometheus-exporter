            "targets": [
               {
                  "datasource": {
                     "type": "prometheus",
                     "uid": "${prometheus_datasource}"
                  },
                  "expr": "increase(ibm_db2_log_operations_total{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}[$__interval:] offset -$__interval)",
                  "format": "time_series",
                  "instant": false,
                  "interval": "1m",
                  "intervalFactor": 2,
                  "legendFormat": "{{database_name}} - {{log_member}} - {{log_operation_type}}",
                  "refId": "Log operations"
               }
            ],
            "title": "Log operations",
            "type": "timeseries"
         }
      ],
      "refresh": "30s",
      "schemaVersion": 39,
      "tags": [
         "ibm-db2-mixin"
      ],
      "templating": {
         "list": [
            {
               "label": "Prometheus data source",
               "name": "prometheus_datasource",
               "query": "prometheus",
               "regex": "(?!grafanacloud-usage|grafanacloud-ml-metrics).+",
               "type": "datasource"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Job",
               "multi": true,
               "name": "job",
               "query": "label_values(ibm_db2_application_active{job=\"integrations/ibm-db2\"}, job)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".*",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Cluster",
               "multi": true,
               "name": "cluster",
               "query": "label_values(ibm_db2_application_active{job=\"integrations/ibm-db2\",job=~\"$job\"}, cluster)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Instance",
               "multi": true,
               "name": "instance",
               "query": "label_values(ibm_db2_application_active{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\"}, instance)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "allValue": ".+",
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "includeAll": true,
               "label": "Database_name",
               "multi": true,
               "name": "database_name",
               "query": "label_values(ibm_db2_application_active{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\"}, database_name)",
               "refresh": 2,
               "sort": 1,
               "type": "query"
            },
            {
               "hide": 2,
               "label": "Loki data source",
               "name": "loki_datasource",
               "query": "loki",
               "regex": "(?!grafanacloud.+usage-insights|grafanacloud.+alert-state-history).+",
               "type": "datasource"
            },
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "${prometheus_datasource}"
               },
               "multi": true,
               "name": "database_name",
               "query": "label_values(ibm_db2_application_active{job=\"integrations/ibm-db2\",job=~\"$job\",cluster=~\"$cluster\",instance=~\"$instance\",database_name=~\"$database_name\"}, database_name)",
               "refresh": 2,
               "type": "query"
            }
         ]
      },
      "time": {
         "from": "now-30m",
         "to": "now"
      },
      "timezone": "default",
      "title": "IBM DB2 overview",
      "uid": "ibmdb2-overview"
   }